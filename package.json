{
  "name": "warp-tauri",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "tauri": "tauri",
    "tauri:dev": "pkill -f warp-tauri 2>/dev/null; lsof -ti :5173 | xargs kill -9 2>/dev/null; sleep 1; tauri dev",
    "tauri:single": "./start-single.sh",
    "tauri:build": "tauri build",
    "tauri:test": "cargo test --manifest-path=src-tauri/Cargo.toml",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "agent:start": "node ./ai_agent_server.cjs",
    "agent:dev": "NODE_ENV=development node ./ai_agent_server.cjs",
    "agent:enhanced": "node ./ai_agent_server_enhanced.cjs",
    "discover:llm": "chmod +x ./llm_app_discovery.sh && ./llm_app_discovery.sh",
    "sync:watch": "node ./syncWatcher.js",
    "test:desktop": "node desktop_automation.cjs --app ChatGPT --prompt 'Say hello'",
    "test:escalation": "playwright test tests/ui/e2e/escalation-desktop.spec.ts",
    "chatgpt": "node chatgpt_web_stealth.cjs",
    "chatgpt:login": "node chatgpt_web_stealth.cjs --login",
    "chatgpt:threads": "node chatgpt_thread_manager.cjs list",
    "chatgpt:orchestrate": "node chatgpt_orchestrator.cjs --interactive",
    "chatgpt:watch": "node chatgpt_session_watcher.cjs",
    "loop": "node autonomous_loop.cjs",
    "loop:claude": "node claude_task_runner.cjs",
    "autonomous": "./start_autonomous_dev.sh",
    "maintainer:notify": "npx ts-node scripts/maintainer/notification-agent.ts",
    "maintainer:tag": "npx ts-node scripts/maintainer/auto-tagger.ts",
    "maintainer:respond": "npx ts-node scripts/maintainer/response-suggester.ts",
    "maintainer:metrics": "npx ts-node scripts/maintainer/metrics-dashboard.ts",
    "remote": "node scripts/remote-server.cjs",
    "remote:start": "node scripts/remote-server.cjs &",
    "start": "npm run remote:start && npm run tauri:dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@tauri-apps/api": "^1.5.0",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "dompurify": "^3.3.1",
    "marked": "^17.0.1",
    "monaco-editor": "^0.52.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^13.0.0",
    "vue": "^3.3.4",
    "vuedraggable": "^4.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tauri-apps/cli": "^1.5.0",
    "@types/node": "^20.19.27",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^16.6.1",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "sharp": "^0.34.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^4.0.15",
    "ws": "^8.18.3"
  }
}
